{
	"compilerOptions": 
    {
		"target": "es2019",
		"module": "commonjs",
		"outDir": "./dist",
		"rootDir": "./",
		"strict": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
        "baseUrl": ".",
		"paths": 
        {
			"@/*": ["./*"]
		}
	}
}